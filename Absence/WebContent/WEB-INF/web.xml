		<servlet-class>com.sdz.servlets.ModifierListe_Prof</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>modification_absence</servlet-name>
		<url-pattern>/Profs/modifierListe</url-pattern>
	</servlet-mapping>

	<!-- Afficher et Modifier Liste d'absence -->
	<servlet>
		<servlet-name>afficher_modifier_list</servlet-name>
		<servlet-class>com.sdz.servlets.ListModif_Prof</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>afficher_modifier_list</servlet-name>
		<url-pattern>/Profs/afficher_modifier_List</url-pattern>
	</servlet-mapping>
	
