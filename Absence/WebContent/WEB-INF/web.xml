		<servlet-class>com.sdz.servlets.ListEtdNotes_Prof</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>listEtd_attrNote</servlet-name>
		<url-pattern>/Profs/ListEtd_attrNotes</url-pattern>
