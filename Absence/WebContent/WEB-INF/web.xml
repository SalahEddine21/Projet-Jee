	<servlet>
		<servlet-name>Connexion</servlet-name>
		<servlet-class>com.sdz.servlets.ConnexionProfs</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Connexion</servlet-name>
		<url-pattern>/Profs/connexion</url-pattern>
	</servlet-mapping> 
	 
	<!-- Profil -->
	<servlet>
